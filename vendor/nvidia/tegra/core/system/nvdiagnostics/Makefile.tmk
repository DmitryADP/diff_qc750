ifdef NV_COMPONENT_FLAG_STATIC_LIBRARY_SECTION
include $(NV_BUILD_START_COMPONENT)

NV_COMPONENT_NAME              := libnvdiagnostics
NV_COMPONENT_OWN_INTERFACE_DIR := .
ifeq ($(NV_TARGET_SOC),t30)
NV_COMPONENT_SOURCES           := \
	sd/nvddk_sd_block_driver_test_t30.c \
	se/se_test_t30.c \
	pwm/pwm_test.c \
	disp/dsi/nvddk_dsi_block_driver_test.c
else
NV_COMPONENT_SOURCES           := \
	sd/nvddk_sd_block_driver_test_stub.c \
	se/se_test_stub.c \
	pwm/pwm_test_stub.c \
	disp/dsi/nvddk_dsi_block_driver_test_stub.c
endif

NV_COMPONENT_INCLUDES          := \
        pwm/$(NV_TARGET_SOC)

include $(NV_BUILD_STATIC_LIBRARY)
endif

