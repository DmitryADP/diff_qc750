ifdef NV_COMPONENT_FLAG_STATIC_LIBRARY_SECTION
include $(NV_BUILD_START_COMPONENT)

NV_COMPONENT_NAME              := libnv3p
NV_COMPONENT_OWN_INTERFACE_DIR := .
NV_COMPONENT_SOURCES           := \
	nv3p.c \
	nv3p_transport_device.c \
	nv3p_transport_usb_descriptors.c \
	nvboot_misc_ap20.c \
	nvboot_misc_t30.c \
	nvml_usbf_ap20.c \
	nvml_usbf_t30.c
NV_COMPONENT_INCLUDES          := \
	../../drivers/nvboot \
	../../drivers/hwinc

ifeq ($(NV_BUILD_CONFIGURATION_IS_EMULATION),1)
NV_COMPONENT_CFLAGS            += -DNVODM_BOARD_IS_FPGA=1
else
NV_COMPONENT_CFLAGS            += -DNVODM_BOARD_IS_FPGA=0
endif

NV_COMPONENT_HAS_BROKEN_CODE := ignore-all

include $(NV_BUILD_STATIC_LIBRARY)
endif #NV_COMPONENT_FLAG_STATIC_LIBRARY_SECTION

