#
# Build tools in this repository
#
NV_REPOSITORY_BUILD_TOOLS := \
	utils/nvidl
#
# Components in this repository
#
NV_REPOSITORY_COMPONENTS := \
	drivers/nvtestio \
	drivers/nvddk/disp \
	drivers/nvrm/graphics \
	drivers/nvrm/nvrmkernel \
	drivers/nvodm/disp \
	drivers/nvodm/misc \
	drivers/nvodm/dtvtuner \
	drivers/nvodm/query \
	mobile_linux/daemons/nv_hciattach \
	utils/aes_keysched_lock \
	utils/nvos \
	utils/nvapputil \
	utils/nvtestresults

#
# Platform specific components in this repository
#
ifeq ($(NV_BUILD_CONFIGURATION_IS_ANDROID),1)
NV_REPOSITORY_COMPONENTS += \
	drivers/libnvdc \
	utils/nvos/cplconnectclient \
	utils/tegrastats
else
# L4T
NV_REPOSITORY_COMPONENTS += \
	drivers/nvddk/blockdev \
	drivers/nvddk/kbc \
	drivers/nvddk/nand \
	drivers/nvddk/sdio \
	drivers/nvddk/se \
	drivers/nvddk/snor \
	drivers/nvddk/spi_flash \
	drivers/nvddk/usb

ifeq ($(NV_BUILD_CONFIGURATION_IS_QNX),1)
NV_REPOSITORY_COMPONENTS += \
	drivers/libnvdc/qnx
else
NV_REPOSITORY_COMPONENTS += \
	drivers/libnvdc \
	mobile_linux/daemons/nvrm/idlhelper \
	utils/ad1937 \
	utils/tegrastats
endif
endif

