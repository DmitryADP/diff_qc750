ifdef NV_COMPONENT_FLAG_STATIC_LIBRARY_SECTION
include $(NV_BUILD_START_COMPONENT)

NV_COMPONENT_NAME              := libnvos_aos_swi
NV_COMPONENT_OWN_INTERFACE_DIR := .
NV_COMPONENT_SOURCES           := \
	../aos/nvos_aos_swi.c
NV_COMPONENT_CFLAGS            += \
	-DNVOS_ADVANCED_DEBUG=0

ifeq ($(NV_TARGET_SOC),ap20)
NV_COMPONENT_CFLAGS            += \
	-DCONFIG_PLLP_BASE_AS_408MHZ=0
else
NV_COMPONENT_CFLAGS            += \
	-DCONFIG_PLLP_BASE_AS_408MHZ=1
endif

NV_COMPONENT_FLAG_USES_GENERATED_FILES :=1

nvos_swi_client.h: $(NV_SOURCE)/core-private/make/swigen.py
	python $< -client NvOs >$@

nvmm_swi_client.h: $(NV_SOURCE)/core-private/make/swigen.py
	python $< -client NvMM >$@

include $(NV_BUILD_STATIC_LIBRARY)
endif

