	armemulator/armdismemory.c \
	armemulator/armemuhelp.c \
	armemulator/armdisexecute.c \
	armemulator/armemu.c \
	armemulator/armemumemory.c \
	armemulator/armdishelp.c \
	armemulator/armemuexecute.c \
	armemulator/armemuvfpexecute.c

