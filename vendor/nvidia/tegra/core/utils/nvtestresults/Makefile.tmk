_common_broken_code_flags      := \
	sign-compare \
	missing-field-initializers

# Static lib
ifdef NV_COMPONENT_FLAG_STATIC_LIBRARY_SECTION
include $(NV_BUILD_START_COMPONENT)

NV_COMPONENT_NAME              := libnvtestresults
NV_COMPONENT_OWN_INTERFACE_DIR := .
NV_COMPONENT_SOURCES           := \
	nvresults.c

ifeq ($(NV_BUILD_CONFIGURATION_IS_ANDROID),1)
NV_COMPONENT_HAS_BROKEN_CODE   := \
	$(_common_broken_code_flags)
endif

include $(NV_BUILD_STATIC_LIBRARY)
endif

# Shared lib
ifdef NV_COMPONENT_FLAG_SHARED_LIBRARY_SECTION
include $(NV_BUILD_START_COMPONENT)

NV_COMPONENT_NAME              := nvtestresults
NV_COMPONENT_OWN_INTERFACE_DIR := .
NV_COMPONENT_SOURCES           := \
	nvresults.c
NV_COMPONENT_NEEDED_INTERFACE_DIRS := \
	$(NV_SOURCE)/core/utils/nvos \
	$(NV_SOURCE)/core/drivers/nvtestio

ifeq ($(NV_BUILD_CONFIGURATION_IS_ANDROID),1)
NV_COMPONENT_HAS_BROKEN_CODE   := \
	$(_common_broken_code_flags)
endif

include $(NV_BUILD_SHARED_LIBRARY)
endif

# Local variable cleanup
_common_broken_code_flags :=

