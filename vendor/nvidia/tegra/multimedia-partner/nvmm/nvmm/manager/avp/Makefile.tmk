include $(NV_BUILD_START_COMPONENT)

NV_COMPONENT_NAME     := nvmm_manager
NV_COMPONENT_SOURCES  := nvmm_manager_avp.c
NV_COMPONENT_INCLUDES := ../../include

NV_COMPONENT_NEEDED_STATIC_INTERFACE_DIRS := \
	../../avp \
	$(NV_SOURCE)/core/utils/nvos/avp \
	$(NV_SOURCE)/core-private/drivers/nvrm/avp

