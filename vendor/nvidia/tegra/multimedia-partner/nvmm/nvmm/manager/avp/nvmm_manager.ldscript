NVAVPNM_IRAM_PREF 0x0000000 RELOC
{
	PREF_ROOT +0x0 
	{
            *(+RO, +RW,+ZI)
	}
}

NVAVPNM_IRAM_MAND 0x40000000 RELOC
{
	MAND_ROOT +0x0 
	{
            *.o(NVAVPNM_IRAM_MAND)
	}
}

