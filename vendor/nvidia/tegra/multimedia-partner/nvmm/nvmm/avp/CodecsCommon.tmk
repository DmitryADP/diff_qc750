# This is how it is currently handled in LDK, that is, all unresolved
# symbols are mapped to AxfModuleMain
NV_COMPONENT_LDFLAGS               += \
	--entry AxfModuleMain \
	--unresolved AxfModuleMain

NV_COMPONENT_INCLUDES              := \
	$(NV_SOURCE)/multimedia-partner/nvmm/include \
	$(NV_SOURCE)/multimedia-partner/nvmm/nvmm/include \
	$(NV_SOURCE)/core-private/drivers/hwinc \
	$(NV_SOURCE)/core-private/include

NV_COMPONENT_NEEDED_STATIC_INTERFACE_DIRS := \
	$(NV_SOURCE)/core-private/drivers/nvrm/avp \
	$(NV_SOURCE)/multimedia-partner/nvmm/nvmm/avp \
	$(NV_SOURCE)/core/utils/nvos/avp

NV_COMPONENT_NEEDED_WHOLE_STATIC_INTERFACE_DIRS := \
	$(NV_SOURCE)/multimedia-partner/nvmm/nvmm/transport

NV_COMPONENT_SCATTER_FILE := $(NV_SOURCE)/multimedia/codecs/nvmm_block.ldscript

NV_COMPONENT_SYSTEMIMAGE_DIR := $(NV_SYSTEMIMAGE_FIRMWARE_DIR)

